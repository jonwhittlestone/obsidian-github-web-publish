{
	"name": "obsidian-github-web-publish",
	"version": "0.1.0",
	"description": "Publish notes to Jekyll/GitHub Pages via PR workflow",
	"main": "main.js",
	"type": "module",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"lint": "eslint .",
		"test": "vitest run",
		"test:watch": "vitest"
	},
	"keywords": [
		"obsidian",
		"plugin",
		"github",
		"jekyll",
		"publishing"
	],
	"author": "Jon Whittlestone",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^20.0.0",
		"esbuild": "0.25.5",
		"eslint-plugin-obsidianmd": "0.1.9",
		"globals": "14.0.0",
		"tslib": "2.4.0",
		"typescript": "^5.8.3",
		"typescript-eslint": "8.35.1",
		"@eslint/js": "9.30.1",
		"jiti": "2.6.1",
		"vitest": "^1.0.0"
	},
	"dependencies": {
		"obsidian": "latest",
		"@octokit/core": "^5.0.0"
	}
}
